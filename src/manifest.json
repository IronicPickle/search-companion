{
  "name": "Search Companion",
  "version": "1.0.0",
  "description": "Search Companion",
  "permissions": [ "storage", "webNavigation", "<all_urls>" ],
  "background": {
    "scripts": [ "js/background/main.js" ],
    "persistent": false
  },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [ "js/content/main.js", "js/react/embed.js" ]
  }, {
    "matches": [ "https://indexcms.co.uk/2.7/case-management*" ],
    "js": [ "js/content/interfaces/cms.js" ]
  }, {
    "matches": [ "https://www.terrafirmaidc.co.uk/order/order_report*",
      "https://www.terrafirmaidc.co.uk/order"
    ],
    "js": [ "js/content/interfaces/terra.js" ]
  }],
  "options_page": "html/options.html",
  "page_action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}